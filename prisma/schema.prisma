generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TelegramUser {
  id            Int      @id @default(autoincrement())
  tgId          Int      @unique
  firstName     String?
  lastName      String?
  username      String?
  languageCode  String?
  firstVisit    DateTime @default(now())
  lastVisit     DateTime @default(now())
  visitCount    Int      @default(1)
} 